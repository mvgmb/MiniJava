class UltimateCode {
    public static void main(String[] a) {
	    System.out.println(new BBS().Start(10));
    }
}

// ################ new Class
class Borba {
    int b1;
    Borba sad;

    public int get() {
       sad = new Borba() ;
       return b1;
    }
}

class Malu extends Borba {
    public int get(int a, int b) {
        return b1;
    }
}
class BBS {

    int[] number ;
    int size ;
    boolean b ;
    Borba borba ;

    public int Start(int sz){
        int aux ;
        borba = new Borba() ;
        number = new int[12];
        aux = new Malu().get(12,12);
        aux = this.Print();
        aux = this.Sort();
        aux = this.Print();
        return aux ;
    }

    public int Sort(){
        if ((size + (number.length) < 4) && (size - 2 < 1))
            System.out.println(1);
        else
            size = size + 1;

        while (!true) {
            b = true ;
        }
        number[1] = 2 ;
        return 42 ;
    }

    public int Print(){
        int j ;
        j = 0 ;
        while (!(j < (size))) {
            System.out.println(number[j]);
            j = j + 1 ;
        }
	    return 0 ;
    }

    public int Init(int sz){
    	size = sz ;
	    number = new int[sz] ;

	    number[0] = 20 ;
	    return 0 ;
    }

}

